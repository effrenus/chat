{
  "name": "vjs",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean -s",
    "prestart": "npm run build",
    "start": "npm run server:prod",
    "server:prod": "NODE_ENV=production node index.js",
    "server:dev": "npm run clean && npm run copy:js && NODE_ENV=develop DEBUG=socket.io* node index.js",
    "test": "mocha $(find test -name '*.js') --sort",
    "test:web": "mocha --compilers js:babel/register,sass:./tests/sass-compiler.js --recursive ./tests/web",
    "build": "npm run clean && npm run copy:js && webpack --config ./webpack.config.js --progress --colors",
    "lint": "eslint ./**/*.{js,jsx}",
    "copy:js": "mkdir -p static/js/libs && cp -r client/libs/* static/js/libs/",
    "clean": "rm -rf static/js static/css static/app.js"
  },
  "pre-commit": [
    "lint"
  ],
  "engines": {
    "node": "4.x"
  },
  "homepage": "https://vanilla-chat.herokuapp.com/",
  "dependencies": {
    "body-parser": "^1.0.2",
    "classnames": "^2.1.5",
    "compression": "^1.6.0",
    "concat-with-sourcemaps": "^1.0.4",
    "config": "^1.16.0",
    "connect-mongo": "^0.8.2",
    "connect-mongodb": "^1.1.5",
    "cookie": "^0.2.2",
    "cookie-parser": "^1.0.1",
    "debug": "~2.2.0",
    "errorhandler": "^1.4.2",
    "express": "~4.13.3",
    "express-session": "^1.11.3",
    "forever": "^0.15.1",
    "forever-monitor": "^1.7.0",
    "inherit": "^2.2.2",
    "jade": "^1.11.0",
    "marked": "^0.3.5",
    "mime": "~1.3.4",
    "mongoose": "^4.1.7",
    "morgan": "^1.0.1",
    "nconf": "^0.8.2",
    "object-assign": "^4.0.1",
    "passport": "^0.3.0",
    "passport-facebook": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-vkontakte": "^0.4.0",
    "peer": "^0.2.8",
    "react": "^0.13.3",
    "react-redux": "^3.1.0",
    "redux": "^3.0.0",
    "redux-thunk": "^1.0.0",
    "require-directory": "^2.1.1",
    "socket.io": "^1.3.6",
    "socket.io-client": "^1.3.6",
    "socket.io-handshake": "0.0.10",
    "through2": "^2.0.0",
    "winston": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.9.3",
    "classnames": "^2.1.5",
    "css-loader": "^0.18.0",
    "del": "^2.0.2",
    "eslint": "^1.5.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-plugin-react": "^3.4.2",
    "expect": "^1.11.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "form-serialize": "^0.6.0",
    "history": "^1.11.1",
    "jsdom": "^6.5.1",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "moment": "^2.10.6",
    "node-sass": "^3.3.2",
    "peerjs": "^0.3.14",
    "pre-commit": "^1.1.1",
    "react-router": "^1.0.0-rc1",
    "redux-router": "^1.0.0-beta3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.10.1",
    "webpack-hot-middleware": "^2.4.1"
  },
  "private": true
}
