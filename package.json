{
  "name": "vjs",
  "version": "1.1.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean -s",
    "prestart": "npm run build",
    "start": "npm run server:prod",
    "server:prod": "NODE_ENV=production node index.js",
    "server:dev": "npm run clean && npm run copy:js && NODE_ENV=develop DEBUG=socket.io* node index.js",
    "test": "mocha $(find test -name '*.js') --sort",
    "test:web": "mocha --compilers js:babel/register,sass:./tests/sass-compiler.js --recursive ./tests/web",
    "build": "npm run clean && npm run copy:js && webpack --config ./webpack.config.js --progress --colors",
    "lint": "eslint ./**/*.{js,jsx}",
    "copy:js": "mkdir -p static/js/libs && cp -r client/libs/* static/js/libs/",
    "clean": "rm -rf static/js static/css static/app.js"
  },
  "pre-commit": [
    "lint"
  ],
  "engines": {
    "node": "4.x"
  },
  "homepage": "https://vanilla-chat.herokuapp.com/",
  "dependencies": {
    "body-parser": "^1.14.1",
    "classnames": "^2.2.0",
    "compression": "^1.6.0",
    "config": "^1.16.0",
    "connect-mongo": "^0.8.2",
    "connect-mongodb": "^1.1.5",
    "cookie": "^0.2.3",
    "cookie-parser": "^1.4.0",
    "debug": "~2.2.0",
    "errorhandler": "^1.4.2",
    "express": "~4.13.3",
    "express-session": "^1.12.1",
    "forever-monitor": "^1.7.0",
    "inherit": "^2.2.2",
    "jade": "^1.11.0",
    "marked": "^0.3.5",
    "mongoose": "^4.2.3",
    "morgan": "^1.6.1",
    "nconf": "^0.8.2",
    "object-assign": "^4.0.1",
    "passport": "^0.3.0",
    "passport-facebook": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-vkontakte": "^0.4.0",
    "peer": "^0.2.8",
    "react": "^0.14.1",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "require-directory": "^2.1.1",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "socket.io-handshake": "0.0.10",
    "winston": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-preset-es2015": "^6.0.14",
    "babel-preset-react": "^6.0.14",
    "babel-preset-stage-0": "^6.0.14",
    "css-loader": "^0.21.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "expect": "^1.12.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "form-serialize": "^0.7.0",
    "history": "^1.13.0",
    "jsdom": "^7.0.2",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "moment": "^2.10.6",
    "node-sass": "^3.4.1",
    "peerjs": "^0.3.14",
    "pre-commit": "^1.1.2",
    "react-router": "^1.0.0-rc3",
    "redux-router": "^1.0.0-beta3",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.4.1"
  },
  "private": true
}
